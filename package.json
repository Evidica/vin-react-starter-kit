{
  "name": "vin-react-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for creating client-side apps using React at Vinsolutions",
  "main": "main.js",
  "scripts": {
    "start": "gulp",
    "test": "mocha --compilers js:babel/register --recursive \"./src\" --colors",
    "coverage-es5": "istanbul cover --color ./node_modules/mocha/bin/_mocha -- --compilers js:babel/register --recursive \"./src\" --colors  --reporter min",
    "coverage-es6": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive \"./src\" --colors --reporter min",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "browserify": "^11.0.1",
    "flux": "^2.0.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.6",
    "babelify": "^6.3.0",
    "browser-sync": "^2.10.0",
    "chai": "^3.3.0",
    "cheerio": "^0.19.0",
    "del": "^2.1.0",
    "eslint-plugin-react": "^3.10.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^1.1.1",
    "gulp-mocha": "^2.1.3",
    "gulp-open": "^1.0.0",
    "gulp-sass": "^2.0.4",
    "gulp-sequence": "^0.4.1",
    "gulp-shell": "^0.5.0",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.22",
    "minifyify": "^7.1.0",
    "mocha": "^2.3.3",
    "reactify": "^1.1.1",
    "redux-devtools": "^2.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
